{
    "manifest_version": 2,

    "name": "gomTranslator",
    "description": "translates all page",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs", "http://*/*", "https://*/*",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "*://*.google.com/",
        "background"
    ]
}